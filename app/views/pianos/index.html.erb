<%= render template: "/partials/_admin_nav.html.erb" %>

<div class="banner">
  <img src="https://github.com/Jfrederick12/Chicago_Electric_Piano_Site/blob/master/app/assets/images/cepco_logo_white.png?raw=true" height="50%" width="50%" />
</div>

<% @pianos.each do |piano| %>
  <div class="piano-view-container">
    <%= piano.model %>

    <a href="/pianos/<%= piano.id %>/"><img src='<%= piano.photo %>'  /></a>
    <%= piano.description %>

  </div>
<% end %>